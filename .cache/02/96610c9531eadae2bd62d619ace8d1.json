olumn":42}},{"generated":{"line":113,"column":17},"source":"components/ConductTransaction.js","original":{"line":61,"column":44}},{"name":"conductTransaction","generated":{"line":113,"column":22},"source":"components/ConductTransaction.js","original":{"line":61,"column":49}},{"generated":{"line":114,"column":0},"source":"components/ConductTransaction.js","original":{"line":61,"column":10}},{"generated":{"line":114,"column":18},"source":"components/ConductTransaction.js","original":{"line":60,"column":8}},{"generated":{"line":114,"column":19},"source":"components/ConductTransaction.js","original":{"line":39,"column":6}},{"generated":{"line":114,"column":20},"source":"components/ConductTransaction.js","original":{"line":38,"column":4}},{"generated":{"line":115,"column":0},"source":"components/ConductTransaction.js","original":{"line":67,"column":3}},{"name":"Component","generated":{"line":119,"column":2},"source":"components/ConductTransaction.js","original":{"line":6,"column":48}}],"sources":{"components/ConductTransaction.js":"import React, { Component } from \"react\";\nimport { FormGroup, FormControl, Button } from \"react-bootstrap\";\nimport { Link } from \"react-router-dom\";\nimport history from \"../history\";\n\nexport default class ConductTransaction extends Component {\n  state = {\n    recipient: \"\",\n    amount: 0,\n  };\n\n  updateRecipient = (event) => this.setState({ recipient: event.target.value });\n\n  updateAmount = (event) =>\n    this.setState({ amount: Number(event.target.value) });\n\n  conductTransaction = () => {\n    const { recipient, amount } = this.state;\n\n    fetch(`${document.location.origin}/api/transact`, {\n      method: \"POST\",\n      headers: {\n        \"Content-Type\": \"application/json\",\n      },\n      body: JSON.stringify({\n        recipient,\n        amount,\n      }),\n    })\n      .then((response) => response.json())\n      .then((json) => {\n        alert(json.message || json.type);\n        history.push(\"/transaction-pool\");\n      });\n  };\n\n  render() {\n    return (\n      <div className='ConductTransaction'>\n        <div>\n          <Link to='/'>Home</Link>\n        </div>\n        <h3>Conduct a Transaction</h3>\n        <FormGroup>\n          <FormControl\n            input='text'\n            placeholder='recipient'\n            value={this.state.recipient}\n            onChange={this.updateRecipient}\n          />\n        </FormGroup>\n        <FormGroup>\n          <FormControl\n            input='number'\n            placeholder='amount'\n            value={this.state.amount}\n            onChange={this.updateAmount}\n          />\n        </FormGroup>\n        <div>\n          <Button bsStyle='danger' onClick={this.conductTransaction}>\n            Submit\n          </Button>\n        </div>\n      </div>\n    );\n  }\n}\n"},"lineCount":null}},"hash":"784a265be23bae05b03e6e04fd676b7f","cacheData":{"env":{}}}